<!-- 产品档案历史表 列表总数-->
<select id="queryByCount" resultType="java.lang.Integer"  parameterType="Object">
	SELECT count(1)
	from
	(select t1.*
	from bas_sku_history t1 left join bas_customer t2 on t1.sku_group6 = t2.CUSTOMERID
	<include refid="Example_Where_Clause_ByPage"/>) as a
	LEFT JOIN bas_customer e on a.customerid = e.CUSTOMERID
	LEFT JOIN bas_customer b ON a.sku_group6=b.customerid
	LEFT JOIN bas_package c ON a.packid=c.packid
	left join product_line d on a.sku_group1 =  d.product_line_id
	left join bas_codes iv on a.reservedfield04 = iv.code
	left join (select * from bas_codes where codeid = 'UOM') uom on uom.code = a.defaultreceivinguom
</select>
  	
<!-- 查询产品档案历史表列表 -->
<select id="queryByList" resultMap="BaseResultMap"  parameterType="Object">

	SELECT a.*,b.descr_c AS skuGroup6Name,e.DESCR_C clientName,c.descr,d.name productLineName,iv.codename_c,uom.codename_c unit
	from
	(select t1.*
	from bas_sku_history t1 left join bas_customer t2 on t1.sku_group6 = t2.CUSTOMERID
	<include refid="Example_Where_Clause_ByPage"/>) as a
	LEFT JOIN bas_customer e on a.customerid = e.CUSTOMERID
	LEFT JOIN (select DISTINCT(customerid),t1.descr_c from bas_customer t1 UNION
	select DISTINCT(customerid),t1.descr_c from bas_customer_history t1) b ON a.sku_group6=b.customerid
	LEFT JOIN bas_package c ON a.packid=c.packid
	left join product_line d on a.sku_group1 =  d.product_line_id
	left join bas_codes iv on a.reservedfield04 = iv.code
	left join (select * from bas_codes where codeid = 'UOM') uom on uom.code = a.defaultreceivinguom
	<if test="orderByClause != null and orderByClause != ''" >
		order by  ${orderByClause}
	</if>
	<if test="limitClause != null and limitClause != ''" >
		${limitClause}
	</if>
</select>
  	
